
/**
 *
 * @author choudhuryb
 */
public class Fibonacci {

    public static void main(String[] args) {
        int number = 10;       
        int [] a = new int [number]; 
        a[0] =0; 
        a[1] =1; 
        for (int i = 2; i < number; i++) {
            a[i]=a[(i-1)]+a[(i-2)];            
        }
        for(int j: a){
            System.out.print(j + ", ");
        }
    }
}
